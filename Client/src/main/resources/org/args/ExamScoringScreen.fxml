<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<ScrollPane fitToHeight="true" fitToWidth="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" stylesheets="@bootstrap3.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.args.GUI.ExamScoringController">
   <content>
      <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
         <children>
            <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
               <children>
                  <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                     <children>
                        <Label text="Question List" />
                        <ListView fx:id="questionListView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" />
                     </children>
                  </VBox>
                  <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                     <children>
                        <Label text="Score List" />
                        <ListView fx:id="scoreListView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" />
                     </children>
                  </VBox>
               </children>
            </HBox>
            <ButtonBar maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
              <buttons>
                  <Label text="Total Score:" />
                  <Label fx:id="scoreLabel" layoutX="230.0" layoutY="22.0" text="Score" />
                  <Button fx:id="backButton" mnemonicParsing="false" onAction="#backButtonClicked" styleClass="danger" text="Back" />
                <Button fx:id="doneButton" mnemonicParsing="false" onAction="#doneButtonOnAction" styleClass="primary" text="Done" />
              </buttons>
            </ButtonBar>
         </children>
      </VBox>
   </content>
</ScrollPane>
