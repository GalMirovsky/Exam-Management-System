<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Pagination?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<ScrollPane fitToHeight="true" fitToWidth="true" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.args.GUI.ViewExamController">
   <content>
      <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
         <children>
            <Pagination fx:id="pageView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
               <VBox.margin>
                  <Insets />
               </VBox.margin></Pagination>
            <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
               <children>
                  <ImageView fx:id="backButton" fitHeight="46.0" fitWidth="54.0" onMouseClicked="#onBackClick" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../Assets/left-arrow.png" />
                     </image>
                  </ImageView>
                  <Label prefHeight="44.0" prefWidth="35.0" text="Back" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="20.0" left="60.0" right="60.0" top="20.0" />
         </padding>
      </VBox>
   </content>
</ScrollPane>
