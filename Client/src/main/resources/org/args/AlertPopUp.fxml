   <children>
      <Label fx:id="showText" alignment="CENTER" prefHeight="105.0" prefWidth="394.0" text="Placeholder" textAlignment="CENTER">
         <VBox.margin>
            <Insets bottom="50.0" top="20.0" />
         </VBox.margin>
         <font>
            <Font name="Verdana" size="24.0" />
         </font>
      </Label>
      <Button fx:id="okBtn" mnemonicParsing="false" onAction="#okBtnAction" prefHeight="32.0" prefWidth="67.0" styleClass="danger" text="OK">
         <VBox.margin>
            <Insets top="50.0" />
         </VBox.margin>
      </Button>
   </children>
   <padding>
      <Insets bottom="50.0" />
   </padding>
</VBox>
