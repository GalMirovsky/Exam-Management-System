<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<StackPane prefHeight="600.0" prefWidth="800.0" stylesheets="@bootstrap3.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.args.GUI.TeacherStatisticsController">
    <VBox alignment="CENTER" spacing="20.0" StackPane.alignment="CENTER">
        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
            <Label fx:id="examTitleLabel" text="EXAM TITLE PLACEHOLDER">
                <font>
                    <Font size="36.0" />
                </font>
            </Label>
        </HBox>
        <VBox alignment="CENTER" prefHeight="317.0" prefWidth="800.0" spacing="20.0">
            <TableView fx:id="gradesTableView" maxHeight="-Infinity" maxWidth="-Infinity" tableMenuButtonVisible="true">
                <columns>
                    <TableColumn fx:id="idColumn" maxWidth="1.7976931348623157E308" prefWidth="414.0" text="Student ID" />
                    <TableColumn fx:id="gradeColumn" maxWidth="1.7976931348623157E308" prefWidth="381.0" text="Grade" />
                </columns>
                <VBox.margin>
                    <Insets left="30.0" right="30.0" />
                </VBox.margin>
            </TableView>
        </VBox>
        <Button fx:id="okButton" mnemonicParsing="false" onAction="#okButtonOnAction" prefHeight="32.0" prefWidth="92.0" styleClass="primary" text="OK" />
    </VBox>
</StackPane>
