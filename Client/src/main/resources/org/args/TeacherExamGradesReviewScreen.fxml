<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<ScrollPane fitToHeight="true" fitToWidth="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.args.GUI.TeacherExamGradesReviewController">
   <content>
      <StackPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" stylesheets="@bootstrap3.css">
         <children>
            <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="10.0">
               <children>
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label fx:id="titleLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="53.0" prefWidth="697.0" text="EXAM TITLE PLACEHOLDER" textAlignment="CENTER">
                           <font>
                              <Font size="36.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" spacing="50.0">
                     <children>
                        <TableView fx:id="gradesTableView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" onMouseClicked="#handleMouseEvent" prefHeight="400.0" prefWidth="333.0">
                           <columns>
                              <TableColumn fx:id="idColumn" maxWidth="1.7976931348623157E308" prefWidth="154.0" text="ID" />
                              <TableColumn fx:id="examTypeColumn" maxWidth="1.7976931348623157E308" prefWidth="178.0" text="Exam Type" />
                           </columns>
                        </TableView>
                        <VBox alignment="CENTER" prefHeight="400.0" prefWidth="135.0" spacing="50.0">
                           <children>
                              <Button fx:id="detailsButton" alignment="CENTER" disable="true" mnemonicParsing="false" onAction="#reviewExam" prefHeight="32.0" prefWidth="93.0" styleClass="info" text="Review" />
                           </children>
                        </VBox>
                     </children>
                  </HBox>
                  <Button fx:id="backButton" mnemonicParsing="false" onAction="#back" text="Back" />
               </children>
               <padding>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </padding>
            </VBox>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </StackPane>
   </content>
</ScrollPane>
